<main class="main">
  <div class="login container">

    <%- include('./components/messages'); -%>
    <%- include('./components/auth-title'); %>

    <div class="clearfix">
      <div class="login-left">
        <h2 class="legend a-center">
          New To UNice
        </h2>
        <div class="register-coupon a-center">
          Get <strong class="mainC">100</strong>
          bonus points for signing up
        </div>
        <form id="register-form" class="register-form" action="/auth/register" method="post">
          <div class="clearfix">
            <div class="input-box half">
              <input name="firstname" type="text" title="Fisrt Name" id="customer_register_firstname" placeholder="First Name" value="<%= typeof firstname !== 'undefined' ? firstname : ''  %>" style="position: absolute; background-image: url(&quot;data:image/png;base64,&quot;); background-repeat: no-repeat; background-attachment: scroll; background-size: 16px 18px; background-position: 98% 50%; cursor: auto;">
            </div>
            <div class="input-box half float-r">
              <input name="lastname" type="text" title="Last Name" id="customer_register_lastname" placeholder="Last Name" value="<%= typeof lastname !== 'undefined' ? lastname : ''  %>" style="position: absolute;">
            </div>
            <div class="input-box">
              <input name="email" type="text" title="Email Address" id="customer_register_email" placeholder="Email" value="<%= typeof email !== 'undefined' ? email : ''  %>" style="position: absolute;">
            </div>
            <div class="input-box half">
              <input name="password" type="password" title="Password" id="customer_register_customer" placeholder="Password" value="<%= typeof password !== 'undefined' ? password : ''  %>" autocomplete="off" style="position: absolute; background-image: url(&quot;data:image/png;base64,&quot;); background-repeat: no-repeat; background-attachment: scroll; background-size: 16px 18px; background-position: 98% 50%;">
            </div>
            <div class="input-box half float-r">
              <input name="confirmPassword" type="password" title="Confirm Password" id="customer_register_confirmation" placeholder="Confirm Password" value="<%= typeof confirmPassword !== 'undefined' ? confirmPassword : ''  %>" autocomplete="off" style="position: absolute; background-image: url(&quot;data:image/png;base64,&quot;); background-repeat: no-repeat; background-attachment: scroll; background-size: 16px 18px; background-position: 98% 50%; cursor: auto;">
            </div>
            <label class="customer-birthday_label" for="month">
              Date of Birth
              <span class="customer-birthday_quest">?</span>
              <p class="customer-birthday_info">Please&nbsp;make&nbsp;sure&nbsp;your&nbsp;birth&nbsp;date&nbsp;is&nbsp;right. Provide&nbsp;your&nbsp;birthday&nbsp;information&nbsp;to&nbsp;allow you&nbsp;to enjoy more considerate services, and we will guarantee your privacy.</p>
            </label>
            <div class="customer-birthday clearfix">
              <select name="month" rel="month" class="customer-birthday_item user-form_select" id="register_month">
                <option value="" selected="selected">Month</option>
                <option value="01">Jan.</option>
                <option value="02">Feb.</option>
                <option value="03">Mar.</option>
                <option value="04">Apr.</option>
                <option value="05">May</option>
                <option value="06">Jun.</option>
                <option value="07">Jul.</option>
                <option value="08">Aug.</option>
                <option value="09">Sep.</option>
                <option value="10">Oct.</option>
                <option value="11">Nov.</option>
                <option value="12">Dec.</option>
              </select>
              <select id="register_day" name="day" rel="day" class="customer-birthday_item user-form_select">
                <option value="" selected="selected">Day</option>
                <option value="01">1</option><option value="02">2</option><option value="03">3</option><option value="04">4</option><option value="05">5</option><option value="06">6</option><option value="07">7</option><option value="08">8</option><option value="09">9</option><option value="10">10</option><option value="11">11</option><option value="12">12</option><option value="13">13</option><option value="14">14</option><option value="15">15</option><option value="16">16</option><option value="17">17</option><option value="18">18</option><option value="19">19</option><option value="20">20</option><option value="21">21</option><option value="22">22</option><option value="23">23</option><option value="24">24</option><option value="25">25</option><option value="26">26</option><option value="27">27</option><option value="28">28</option><option value="29">29</option><option value="30">30</option><option value="31">31</option>
              </select>
              <select id="register_year" name="year" rel="year" class="customer-birthday_item user-form_select">
                <option value="" selected="selected">Year</option>
                <option value="2008">2008</option><option value="2007">2007</option><option value="2006">2006</option><option value="2005">2005</option><option value="2004">2004</option><option value="2003">2003</option><option value="2002">2002</option><option value="2001">2001</option><option value="2000">2000</option><option value="1999">1999</option><option value="1998">1998</option><option value="1997">1997</option><option value="1996">1996</option><option value="1995">1995</option><option value="1994">1994</option><option value="1993">1993</option><option value="1992">1992</option><option value="1991">1991</option><option value="1990">1990</option><option value="1989">1989</option><option value="1988">1988</option><option value="1987">1987</option><option value="1986">1986</option><option value="1985">1985</option><option value="1984">1984</option><option value="1983">1983</option><option value="1982">1982</option><option value="1981">1981</option><option value="1980">1980</option><option value="1979">1979</option><option value="1978">1978</option><option value="1977">1977</option><option value="1976">1976</option><option value="1975">1975</option><option value="1974">1974</option><option value="1973">1973</option><option value="1972">1972</option><option value="1971">1971</option><option value="1970">1970</option><option value="1969">1969</option><option value="1968">1968</option><option value="1967">1967</option><option value="1966">1966</option><option value="1965">1965</option><option value="1964">1964</option><option value="1963">1963</option><option value="1962">1962</option><option value="1961">1961</option><option value="1960">1960</option><option value="1959">1959</option><option value="1958">1958</option><option value="1957">1957</option><option value="1956">1956</option><option value="1955">1955</option><option value="1954">1954</option><option value="1953">1953</option><option value="1952">1952</option><option value="1951">1951</option><option value="1950">1950</option><option value="1949">1949</option><option value="1948">1948</option><option value="1947">1947</option><option value="1946">1946</option><option value="1945">1945</option><option value="1944">1944</option><option value="1943">1943</option><option value="1942">1942</option><option value="1941">1941</option><option value="1940">1940</option><option value="1939">1939</option><option value="1938">1938</option><option value="1937">1937</option><option value="1936">1936</option><option value="1935">1935</option><option value="1934">1934</option><option value="1933">1933</option><option value="1932">1932</option><option value="1931">1931</option><option value="1930">1930</option><option value="1929">1929</option><option value="1928">1928</option><option value="1927">1927</option><option value="1926">1926</option><option value="1925">1925</option><option value="1924">1924</option><option value="1923">1923</option><option value="1922">1922</option><option value="1921">1921</option><option value="1920">1920</option>
              </select>
            </div>
            <input type="hidden" id="dob" name="dob" value="">
            <div class="check-box inline-box">
              <input id="lateuse" class="checkbox" name="is_accept" type="checkbox" title="" checked="checked" value="1">
              <label class="checkbox-label" for="lateuse">
                  I accept
                  <a class="a-line" href="https://www.unice.com/terms-and-conditions"> Terms of Usage</a>
                  and
                  <a class="a-line" href="https://www.unice.com/privacy-policy">Privacy Policy</a>
                  of UNice.com
              </label>
            </div>
          </div>
          <div class="buttons-set a-center">
            <button class="register-submit button" type="submit">Submit</button>
          </div>
        </form>
      </div>
      <div class="login-right">
        <div class="oldlogin a-center">
          <h2>Return To Login</h2>
          <p>If you have an account, please log in!</p>
          <a class="login-btn button" href="/auth/login">Log in</a>
        </div>
      </div>
    </div>
  </div>
</main>
