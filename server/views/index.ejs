</div>
<% if (typeof brands !== 'undefined') { %>
  <% brands.forEach((brand) => { %>
    <div class="pro-show bettyou clearfix wrapper">
      <div class="pro-show_top">
        <a
          class="pro-show_title img-block"
          href="/brand/<%= brand.name %>"
        >
          <img
            alt="<%= brand.name %>"
            src="<%= brand.topImg %>"
          />
        </a>
        <video
          class="pro-show_video lozad"
          controls="controls"
          data-mp4="true"
          poster="https://xcdn.unice.com/media/ads/default/WIGCOVER.jpg"
        >
          <source
            data-src="https://vcdn.unice.com/zhutuvideo/169wigvideo-01.mp4"
            type="video/mp4"
          />
        </video>
      </div>
      <div class="pro-show_left">
        <a
          class="pro-show_head bettyou"
          href="/brand/<%= brand.name %>"
          title="Be your own stylist, Be Different!"
        >
          <img src="<%= brand.logo %>" alt="<%= brand.name %>">
        </a>
        <div class="pro-show_brand">
          <%= brand.description %>
        </div>
        <div class="pro-show_classification">
          <h4 class="pro-show_classification-tit">main classification</h4>
          <div class="pro-show_classification-list">
            <a
              class="pro-show_classification-item"
              href="https://www.unice.com/wig-1/bob-wig.html"
              title="Bob Wig"
            >
              <img
                class="lozad"
                data-src="https://www.unice.com/skin/frontend/longqi/pc/images/home/body1.png"
                src="images/lozad-home-icon.jpg"
                alt="Bob Wig"
              />
              <span>Bob Wig</span>
            </a>
            <a
              class="pro-show_classification-item"
              href="https://www.unice.com/wig-1/body-wave-wigs.html"
              title="Body Wave"
            >
              <img
                class="lozad"
                data-src="https://www.unice.com/skin/frontend/longqi/pc/images/home/body2.png"
                src="images/lozad-home-icon.jpg"
                alt="Body Wave"
              />
              <span>Body Wave</span>
            </a>
            <a
              class="pro-show_classification-item"
              href="https://www.unice.com/wig-1/curly-hair-wigs.html"
              title="Curly Hair"
            >
              <img
                class="lozad"
                data-src="https://www.unice.com/skin/frontend/longqi/pc/images/home/curly.png"
                src="images/lozad-home-icon.jpg"
                alt="Curly Hair"
              />
              <span>Curly Hair</span>
            </a>
            <a
              class="pro-show_classification-item"
              href="https://www.unice.com/wig-1/kinky-hair-wig.html"
              title="kinky Curly"
            >
              <img class="lozad"
              data-src="https://www.unice.com/skin/frontend/longqi/pc/images/home/kinky.png"
              src="images/lozad-home-icon.jpg" alt="kinky Curly" ">
              <span>kinky Curly</span>
            </a>
            <a
              class="pro-show_classification-item"
              href="https://www.unice.com/wig-1/water-wave-hair-wigs.html"
              title="Water Wave"
            >
              <img
                class="lozad"
                data-src="https://www.unice.com/skin/frontend/longqi/pc/images/home/kinky.png"
                src="images/lozad-home-icon.jpg"
                alt="Water Wave"
              />
              <span>Water Wave</span>
            </a>
            <a
              class="pro-show_classification-item"
              href="https://www.unice.com/wig-1/straight-hair-wigs.html"
              title="Straight Hair"
            >
              <img
                class="lozad"
                data-src="https://www.unice.com/skin/frontend/longqi/pc/images/home/straight.png"
                src="images/lozad-home-icon.jpg"
                alt="Straight Hair"
              />
              <span>Straight Hair</span>
            </a>
          </div>
        </div>
      </div>
      <div class="pro-show_right clearfix">
        <% brand.product.forEach((product) => { %>
          <%- include('./components/home/product', { product }) %>
        <% }) %>
      </div>
    </div>
  <% }) %>
<% } %>

